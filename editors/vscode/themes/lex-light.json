{
  "name": "Lex Light",
  "type": "light",
  "colors": {
    "editor.background": "#ffffff",
    "editor.foreground": "#000000"
  },
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.inline.raw"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "markup.raw.block"
      ],
      "settings": {
        "foreground": "#000000",
        "background": "#f5f5f5"
      }
    },
    {
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#808080",
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "markup.list"
      ],
      "settings": {
        "foreground": "#808080",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "comment",
        "comment.block"
      ],
      "settings": {
        "foreground": "#b3b3b3"
      }
    },
    {
      "scope": [
        "punctuation.definition.heading"
      ],
      "settings": {
        "foreground": "#808080",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "punctuation.definition.list"
      ],
      "settings": {
        "foreground": "#666666",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "variable.other.definition"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#b3b3b3"
      }
    },
    {
      "scope": [
        "entity.name.section",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#b3b3b3"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "text"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": ""
      }
    }
  ]
}